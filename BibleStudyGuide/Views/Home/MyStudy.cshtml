@model BibleStudyGuide.Models.ListsViews
@{
    ViewBag.Title = "MyStudy";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="container">
    @foreach(var item in Model.AllVerseCategories)
    {
        <div class="col-md-6 text-center well">
            <h3>@Html.DisplayFor(modelItem => item.AuthorName)</h3>

        </div>
    }
</section>

